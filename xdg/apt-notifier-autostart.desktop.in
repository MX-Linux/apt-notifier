[Desktop Entry]
# The autostart desktop entry name will be shown within the list of autostart entries.
Name=MX Updater
# TRANSLATORS:
# The autostart desktop entry comment will be shown within 'Session and Startup' (Autosart).
Comment=An applet to show updates
Exec=sh -c "sleep 6; exec /usr/bin/apt-notifier"
Icon=apt-notifier
StartupNotify=false
Terminal=false
Type=Application

