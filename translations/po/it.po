msgstr "$count aggiornamenti disponibili"

msgid "Unattended-upgrades"
msgstr "Aggiornamenti automatici"

msgid "Using basic upgrade (not recommended)"
msgstr "Utilizzando l'aggiornamento base (non raccomandato)"

msgid "Using full upgrade"
msgstr "Utilizzando l'aggiornamento completo"

msgid "upgrade"
msgstr "Aggiorna"

msgid "Upgrade using Synaptic"
msgstr "Aggiorna tramite Synaptic"

msgid "Automatically answer 'yes' to all prompts during full/basic upgrade"
msgstr ""
"Rispondi 'si' automaticamente a tutte le richieste durante l'aggiornamento "
"completo o quello base"

msgid "Automatically answer 'yes' to all prompts during upgrade"
msgstr ""
"Rispondi automaticamente 'si' a tutte le domande durante l'aggiornamento"

msgid "View and Upgrade"
msgstr "Visualizza e Aggiorna"

msgid "Auto-update dpkg log(s)"
msgstr "Auto-aggiorna dpkg log(s)"

msgid "Auto-update log(s)"
msgstr "Auto-aggiorna log(s)"

msgid "MX Updater--View and Upgrade, previewing: basic upgrade"
msgstr ""
"MX Aggiornamenti - Visualizza e Aggiorna, anteprima: aggiornamento base"

msgid "MX Updater--View and Upgrade, previewing: full upgrade"
msgstr ""
"MX Aggiornamenti - Visualizza e Aggiorna, anteprima: aggiornamento completo"

