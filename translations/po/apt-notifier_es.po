msgstr "MX Actualizador Ayuda"

#: apt-notifier.py:74
msgid "Synaptic Help"
msgstr "Ayuda de Synaptic"

#: apt-notifier.py:75
msgid "Preferences"
msgstr "Preferencias"

#: apt-notifier.py:76 apt-notifier.py:1235
msgid "History"
msgstr "Historia"

#: apt-notifier.py:77
msgid "Check for Updates"
msgstr "Buscar actualizaciones"

